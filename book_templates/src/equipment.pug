listresult 
    .template.grid.gap-2
        .header.grid
            h1.primary.font-h1 
                |{{Name}}
            i.font-base.third {{#tags}} {{tags}} {{/tags}}
        hr.third
        .body.font-base.third.grid
            each val in ["ant", "apc", "ar", "attribute", "cost", "damage", "damage_type", "description", "dex_max", "fort", "log", "range", "ref", "resist", "size", "spd", "str_min", "subcategory", "type", "uses", "will"]
                | {{##{val}}}
                .grid.grid-flow-col.gap-2.col-auto-fr
                    span.font-bold.capitalize #{val.replace(/_/g, ' ')}:
                    span.capitalize {{#{val}}}  
                | {{/#{val}}} 