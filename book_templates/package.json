{
  "name": "booktemplate",
  "version": "1.0.0",
  "description": "Roll20 compendium book template",
  "main": "index.js",
  "devDependencies": {
    "jstransformer-uglify-es": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "jq": "^1.7.2"
  },
  "scripts": {
    "dev": "run-p --race  \"build:pug -- -w\" \"build:sass -- --watch\"",
    "build": "run-p --race build:pug build:sass",
    "build:pug": "pug -o ./html ./src",
    "build:sass": "sass --no-source-map --style compressed ./styles.scss ./styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasireal-publishing/roll20-mythcraft"
  },
  "keywords": [
    "Roll20"
  ],
  "author": "Caden Levett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quasireal-publishing/roll20-mythcraft"
  },
  "homepage": "https://github.com/quasireal-publishing/roll20-mythcraft"
}
